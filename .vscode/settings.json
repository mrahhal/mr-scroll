{
  "files.exclude": {
    "**/node_modules": true
  },
  "search.exclude": {
    "**/dist": true
  },
  "htmlhint.options": {
    "attr-lowercase": false,
    "attr-no-duplication": true,
    "attr-value-double-quotes": true,
    "id-unique": true,
    "spec-char-escape": false,
    "doctype-first": false,
    "src-not-empty": true,
    "tag-pair": true,
    "tagname-lowercase": false,
    "title-require": true
  }
}
